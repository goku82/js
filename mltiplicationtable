<h1>Multiplication Table</h1>
<script>
var table = 9;
var length = 10;
var i = 1;
document.write("Multiplication table: "+ table);
for(i=1; i <= length; i++)
document.write("<br>"+i+" * "+table+" = " +(i * table));
</script>
